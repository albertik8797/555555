<Window x:Class="CalendarApp.FoodCategoryWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Выберите категорию еды" Height="200" Width="300">
    <Grid>
        <ListBox x:Name="foodCategoryListBox" HorizontalAlignment="Center" VerticalAlignment="Center" Height="116" Margin="81,20,81,33">
            <ListBoxItem>Фрукты</ListBoxItem>
            <ListBoxItem>Овощи</ListBoxItem>
            <ListBoxItem>Мясо</ListBoxItem>
            <ListBoxItem>Молочные продукты</ListBoxItem>
            <ListBoxItem>Зерновые</ListBoxItem>
        </ListBox>

        <Button x:Name="okButton" Content="OK" HorizontalAlignment="Center" VerticalAlignment="Bottom" Margin="0,10" Width="60" Click="okButton_Click"/>
    </Grid>
</Window>
